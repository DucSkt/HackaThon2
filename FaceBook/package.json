{
  "name": "FaceBook",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "precommit": "pretty-quick --staged",
    "prettier": "prettier --write 'app/**/*.{js,jsx,ts,tsx}'",
    "postinstall": "react-native-schemes-manager all",
    "open-dir": "open app/build/outputs/apk/release",
    "build-dev": "cd android && ENVFILE=.env.dev ./gradlew assembleDevRelease",
    "build-stag": "cd android && ENVFILE=.env.staging ./gradlew assembleStagingRelease",
    "build-prod": "cd android && ENVFILE=.env.production ./gradlew assembleProductionRelease",
    "run-dev": "ENVFILE=.env.dev && react-native run-android --variant=devDebug",
    "run-sta": "ENVFILE=.env.staging && react-native run-android --variant=stagingDebug",
    "run-prod": "ENVFILE=.env.production && react-native run-android --variant=productionDebug",
    "run-dev-ios": "ENVFILE=.env.dev && react-native run-ios",
    "run-sta-ios": "ENVFILE=.env.staging react-native run-ios",
    "run-prod-ios": "react-native run-ios --configuration Debug",
    "debug-android": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:9090 tcp:9090",
    "fix-3": "cd node_modules/react-native/scripts && ./ios-install-third-party.sh && cd ../../../ && cd node_modules/react-native/third-party/glog-0.3.5/ && ../../scripts/ios-configure-glog.sh && cd ../../../../"
  },
  "dependencies": {
    "prettier": "^1.18.2",
    "react": "16.8.3",
    "react-native": "0.59.9",
    "react-native-console-time-polyfill": "^1.2.1",
    "react-native-gesture-handler": "^1.3.0",
    "react-navigation": "^3.11.0",
    "schedule": "^0.5.0",
    "tslint-config-airbnb": "^5.11.1"
  },
  "xcodeSchemes": {
    "Debug": [
      "Dev2.Debug",
      "Dev3.Debug",
      "Staging.Debug",
      "Debug"
    ],
    "Release": [
      "Dev2.Release",
      "Dev3.Release",
      "Staging.Release",
      "Release"
    ],
    "projectDirectory": "iOS"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "@types/react": "^16.8.20",
    "@types/react-native": "^0.57.63",
    "babel-jest": "^24.8.0",
    "eslint-plugin-import": "^2.17.3",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "react-native-schemes-manager": "^2.0.0",
    "react-native-typescript-transformer": "^1.2.12",
    "react-test-renderer": "16.8.3",
    "ts-jest": "^24.0.2",
    "tslib": "^1.10.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-etc": "^1.5.3",
    "tslint-react": "^4.0.0",
    "tslint-react-hooks": "^2.1.1",
    "tsutils": "^3.14.0",
    "typescript": "^3.5.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "rnpm": {
    "assets": [
      "./app/assets/fonts/"
    ]
  }
}
